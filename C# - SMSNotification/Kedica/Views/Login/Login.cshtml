@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>Login | SMS</title>
    <meta content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" name="viewport" />
    @Styles.Render("~/Login-CSS")
    <link href="/Content/assets/css/default/theme/blue.css" rel="stylesheet" id="theme" />
    <link rel="icon" href="~/Content/assets/img/citycalambalogo.png" type="image/x-icon">
    <style>
        #frmLogin .parsley-errors-list > li {
            width: 80% !important;
        }
        .LoginTitle {
            font-size: 23px !important;
            font-weight: bold !important;
            text-align: center !important;
            color: black;
            font-family: Garamond;
        }
        
    </style>
</head>
<body class="pace-top bg-white">
    <div id="page-loader" class="fade show"><span class="spinner"></span></div>
    <div id="loading_modal" class="modal loading_modal" data-backdrop="static">
        <div class="modal-dialog modal-sm">
            <div class="modal-content">
                <div class="modal-body">
                    <div class="row">
                        <div class="col-md-12 text-center">
                            <img src="~/Content/assets/img/ajax-loader.gif" height="100" class="block-centered">
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="login login-with-news-feed">
        <div class="news-feed">
            <div class="news-image" style="background-image: url(/Content/assets/img/home.jpg)">
            </div>
            <div class="news-caption">
            </div>
        </div>
        <div class="right-content">
            <div class="login-header">
                <div class="brand text-center">
                    <img src="~/Content/assets/img/citycalambalogo.png" class="m-b-15" style="height:100px;" /><br />
                </div>
                <div class="icon">
                    <i class="fa fa-sign-in"></i>
                </div>
                <p class="LoginTitle" >CITY COLLEGE OF CALAMBA</p>
                <p  class="LoginTitle" >ANNOUNCEMENT</p>
                <p class="LoginTitle" >SYSTEM</p>
                <div class="icon">
                    <i class="fa fa-sign-in"></i>
                </div>
            </div>
            <div class="login-content">
                <form id="frmLogin" class="margin-bottom-0" data-parsley-validate>
                    @Html.AntiForgeryToken()
                    <div class="form-group m-b-15">
                        <input type="text" data-parsley-errors-container="#err-LoginUsername" id="UserName" name="Username"  class="form-control form-control-lg" placeholder="Email Address" data-parsley-error-message="Please enter your Email Address." required autocomplete="off" />
                    </div>
                    <div id="err-LoginUsername"></div>
                    <div class="form-group m-b-15">
                        <input type="password" data-parsley-errors-container="#err-LoginPassword" id="Password"  name="Password" class="form-control form-control-lg" placeholder="Password" required data-parsley-error-message="Please enter your Password." autocomplete="off" />
                    </div>
                    <div id="err-LoginPassword"></div>
                    <br />
                    <div class="login-buttons">
                        <div class="row">
                            <div class="col-6">
                                <button type="submit"  class="btn btn-success btn-block btn-lg">Sign me in</button>
                            </div>
                            <div class="col-6">
                                <button type="button" id="btnRegisterUser" class="btn btn-success btn-block btn-lg">Register</button>
                            </div>
                        </div>

                    </div>

                    <hr />
                    <p class="text-center text-grey-darker">
                        &copy;  Announcement System via SMS Notification 2022
                    </p>
                </form>
            </div>
        </div>
    </div>


    <div id="page-container" class="fade">

